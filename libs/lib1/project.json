{
  "name": "lib1",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/lib1/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/libs/lib1"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/libs/lib1"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "outputs": [
        "{projectRoot}/{options.coverage.reportsDirectory}",
        "{workspaceRoot}/coverage/libs/lib1"
      ],
      "options": {
        "coverage.reportsDirectory": "../../coverage/libs/lib1"
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/lib1/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/lib1",
        "configDir": "libs/lib1/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
